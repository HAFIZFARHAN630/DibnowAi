    <div class="overflow-hidden rounded-lg shadow-lg">
      <table id="activeUsersTable" class="min-w-full w-full border-collapse">
        <thead class="text-center">
          <tr class="bg-gradient-to-br from-[#667eea] to-[#764ba2] text-white text-sm font-semibold uppercase tracking-wide">
            <th class="px-6 py-3" style=" background: linear-gradient(135deg, #0033cc 0%, #33ccff 100%) !important; "><i class="bi bi-person me-2"></i>Name</th>
            <th class="px-6 py-3" style=" background: linear-gradient(135deg, #0033cc 0%, #33ccff 100%) !important; "><i class="bi bi-envelope me-2"></i>Email</th>
            <th class="px-6 py-3" style=" background: linear-gradient(135deg, #0033cc 0%, #33ccff 100%) !important; "><i class="bi bi-credit-card me-2"></i>Plan</th>
            <th class="px-6 py-3" style=" background: linear-gradient(135deg, #0033cc 0%, #33ccff 100%) !important; "><i class="bi bi-calendar-plus me-2"></i>Start Date</th>
            <th class="px-6 py-3" style=" background: linear-gradient(135deg, #0033cc 0%, #33ccff 100%) !important; "><i class="bi bi-calendar-x me-2"></i>Expiry Date</th>
            <th class="px-6 py-3" style=" background: linear-gradient(135deg, #0033cc 0%, #33ccff 100%) !important; "><i class="bi bi-check-circle me-2"></i>Status</th>
          </tr>
        </thead>

        <tbody class="bg-white">
          <% if (activeUsers && activeUsers.length > 0) { %>
            <% activeUsers.forEach((item) => { %>
              <tr class="hover:bg-gray-50 transition">
                <td class="px-6 py-4">
                  <div class="text-sm font-medium text-gray-800"><%= item.user.first_name %> <%= item.user.last_name %></div>
                </td>

                <td class="px-6 py-4">
                  <div class="text-sm text-gray-600"><%= item.user.email %></div>
                </td>

                <td class="px-6 py-4 text-center">
                  <% let planDisplay = ''; %>
                  <% let planClass = 'plan-default'; %>
                  <% if (item.plan) { %>
                    <% planDisplay = item.plan; %>
                    <% if (item.plan.toLowerCase().includes('free')) { %>
                      <% planClass = 'plan-free-trial'; %>
                    <% } else if (item.plan.toLowerCase().includes('basic')) { %>
                      <% planClass = 'plan-basic'; %>
                    <% } else if (item.plan.toLowerCase().includes('standard')) { %>
                      <% planClass = 'plan-standard'; %>
                    <% } else if (item.plan.toLowerCase().includes('premium')) { %>
                      <% planClass = 'plan-premium'; %>
                    <% } %>
                  <% } else if (item.planName) { %>
                    <% planDisplay = item.planName; %>
                    <% if (item.planName.toLowerCase().includes('free')) { %>
                      <% planClass = 'plan-free-trial'; %>
                    <% } else if (item.planName.toLowerCase().includes('basic')) { %>
                      <% planClass = 'plan-basic'; %>
                    <% } else if (item.planName.toLowerCase().includes('standard')) { %>
                      <% planClass = 'plan-standard'; %>
                    <% } else if (item.planName.toLowerCase().includes('premium')) { %>
                      <% planClass = 'plan-premium'; %>
                    <% } %>
                  <% } else { %>
                    <% planDisplay = 'Free Trial'; %>
                    <% planClass = 'plan-free-trial'; %>
                  <% } %>

                  <span class="flex flex-wrap items-center px-3 py-1 rounded-full text-sm <%= planClass %>"><%= planDisplay %></span>
                </td>

                <td class="px-6 py-4 text-center">
                  <div class="text-sm text-gray-600">
